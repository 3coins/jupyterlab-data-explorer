{
  "name": "@jupyterlab/dataregistry-csvviewer",
  "version": "0.0.0",
  "description": "CSV viewer data converter.",
  "keywords": [
    "jupyter",
    "jupyterlab",
    "jupyterlab-extension"
  ],
  "homepage": "https://github.com/jupyterlab/jupyterlab-data-explorer",
  "bugs": {
    "url": "https://github.com/jupyterlab/jupyterlab-data-explorer/issues"
  },
  "license": "BSD-3-Clause",
  "author": "Project Jupyter",
  "files": [
    "lib/**",
    "style.css"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "style": "style.css",
  "directories": {
    "lib": "lib/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jupyterlab/jupyterlab-data-explorer.git"
  },
  "scripts": {
    "build": "tsc && yarn run generateSchema",
    "clean": "rimraf lib tsconfig.tsbuildinfo",
    "prepublishOnly": "yarn run clean && yarn run build",
    "watch": "tsc -w --listEmittedFiles",
    "lint": "eslint src/*",
    "generateSchema": "ts-json-schema-generator --no-type-check -f tsconfig.json > lib/datasets-file.schema.json"
  },
  "dependencies": {
    "@jupyterlab/application": "^1.0.0",
    "@jupyterlab/csvviewer": "^1.0.0",
    "@jupyterlab/dataregistry": "^2.0.0",
    "@phosphor/datagrid": "^0.1.10",
    "@phosphor/messaging": "^1.2.3",
    "rxjs": "6.5.2",
    "@jupyterlab/dataregistry-registry": "file:../dataregistry-registry"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "1.13.0",
    "@typescript-eslint/parser": "1.13.0",
    "eslint": "6.1.0",
    "eslint-config-prettier": "6.0.0",
    "eslint-plugin-react": "7.14.3",
    "eslint-plugin-react-hooks": "1.6.1",
    "rimraf": "~2.6.2",
    "ts-json-schema-generator": "0.52.14",
    "typescript": "3.5.3"
  },
  "publishConfig": {
    "access": "public"
  },
  "jupyterlab": {
    "extension": true
  }
}
