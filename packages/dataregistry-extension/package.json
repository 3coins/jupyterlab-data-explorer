{
  "name": "@jupyterlab/dataregistry-extension",
  "version": "4.0.0",
  "description": "Datasets as first class entities in Juptyerlab.",
  "keywords": [
    "jupyter",
    "jupyterlab",
    "jupyterlab-extension"
  ],
  "homepage": "https://github.com/jupyterlab/jupyterlab-data-explorer",
  "bugs": {
    "url": "https://github.com/jupyterlab/jupyterlab-data-explorer/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jupyterlab/jupyterlab-data-explorer.git"
  },
  "license": "BSD-3-Clause",
  "author": "Project Jupyter",
  "files": [
    "lib/**",
    "style.css"
  ],
  "main": "lib/index.js",
  "types": "./lib/index.d.ts",
  "style": "./style.css",
  "directories": {
    "lib": "./lib"
  },
  "scripts": {
    "build": "tsc --build && jlpm run postBuild",
    "postBuild": "jlpm run generateSchema",
    "clean": "rimraf lib tsconfig.tsbuildinfo",
    "generateSchema": "ts-json-schema-generator --type datasetsObjectType --no-type-check -f tsconfig.json > lib/datasets-file.schema.json",
    "prepublishOnly": "npm run clean && npm run build",
    "watch": "tsc --build --watch --listEmittedFiles"
  },
  "dependencies": {
    "@jupyterlab/application": "^3.1.10",
    "@jupyterlab/apputils": "^3.1.10",
    "@jupyterlab/coreutils": "^5.1.10",
    "@jupyterlab/launcher": "^3.1.10",
    "@jupyterlab/services": "^6.1.10",
    "@jupyterlab/settingregistry": "^3.1.10",
    "@jupyterlab/statedb": "^3.1.10",
    "@jupyterlab/translation": "^3.1.10",
    "@jupyterlab/ui-components": "^3.1.10",
    "@lumino/algorithm": "^1.3.3",
    "@lumino/commands": "^1.12.0",
    "@lumino/widgets": "^1.26.0",
    "@jupyterlab/csvviewer": "^2.1.0",
    "@jupyterlab/docmanager": "^3.1.10",
    "@jupyterlab/docregistry": "^3.1.10",
    "@jupyterlab/filebrowser": "^3.1.10",
    "@jupyterlab/notebook": "^3.1.10",
    "@jupyterlab/rendermime": "^3.1.10",
    "@lumino/datagrid": "^0.6.0",
    "@lumino/messaging": "^1.2.3",
    "ajv": "^6.10.2",
    "js-yaml": "^3.13.1",
    "react": "^17.0.1",
    "react-inspector": "^5.0.1",
    "rxjs": "^6.5.2",
    "styled-components": "^5.3.1"
  },
  "devDependencies": {
    "@jupyterlab/dataregistry": "^4.0.0",
    "@types/js-yaml": "^3.12.1",
    "ts-json-schema-generator": "0.65.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "jupyterlab": {
    "extension": true
  }
}
