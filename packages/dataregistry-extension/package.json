    "watch": "tsc -b --watch --listEmittedFiles"
  },
  "dependencies": {
    "@jupyterlab/application": "^1.0.0",
    "@jupyterlab/apputils": "^1.0.0",
    "@jupyterlab/coreutils": "^3.0.0",
    "@jupyterlab/csvviewer": "^1.0.0",
    "@jupyterlab/dataregistry": "^2.0.0",
    "@jupyterlab/docmanager": "^1.0.0",
    "@jupyterlab/docregistry": "^1.0.4",
    "@jupyterlab/filebrowser": "^1.0.0",
    "@jupyterlab/notebook": "^1.0.0",
    "@jupyterlab/rendermime": "^1.0.0",
    "@nteract/data-explorer": "7.0.1",
    "@phosphor/datagrid": "^0.1.10",
    "@phosphor/messaging": "^1.2.3",
    "@phosphor/widgets": "^1.8.0",
    "ajv": "6.10.2",
    "js-yaml": "3.13.1",
    "path": "0.12.7",
    "react": "16.8.4",
    "react-inspector": "3.0.0",
    "rxjs": "6.5.2",
    "styled-components": "4.3.2",
    "@jupyterlab/dataregistry-csvviewer": "file:../dataregistry-csvviewer",
    "@jupyterlab/dataregistry-registry": "file:../dataregistry-registry"
  },
  "devDependencies": {
    "@types/js-yaml": "3.12.1",
    "rimraf": "~2.6.2",
    "ts-json-schema-generator": "0.52.14",
    "typescript": "3.5.3"
