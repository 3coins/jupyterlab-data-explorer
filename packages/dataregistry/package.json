    "unlink": "jupyter labextension unlink ./ --no-build",
    "watch": "tsc --build --watch --listEmittedFiles"
  },
  "dependencies": {
    "rxjs": "^6.5.2",
    "rxjs-spy": "^7.5.1",
    "uri-templates": "0.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@types/jest": "^24.0.13",
    "@types/node": "^8.0.47",
    "@types/uri-templates": "0.1.30",
    "babel-jest": "^24.9.0",
    "ix": "^2.5.3",
    "jest": "^24.8.0",
    "rimraf": "^2.6.2",
    "typescript": "^3.5.3"
  },
  "publishConfig": {
