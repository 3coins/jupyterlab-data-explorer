{
  "name": "@jupyterlab/dataregistry-metapackage",
  "version": "0.0.0",
  "description": "Meta package consisting of all data registry packages.",
  "keywords": [
    "jupyter",
    "jupyterlab",
    "jupyterlab-extension"
  ],
  "homepage": "https://github.com/jupyterlab/jupyterlab-data-explorer",
  "bugs": {
    "url": "https://github.com/jupyterlab/jupyterlab-data-explorer/issues"
  },
  "license": "BSD-3-Clause",
  "author": "Project Jupyter",
  "files": [
    "lib/**"
  ],
  "main": "./lib",
  "types": "./lib/index.d.ts",
  "directories": {
    "lib": "./lib"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jupyterlab/jupyterlab-data-explorer.git"
  },
  "scripts": {
    "build": "tsc --build",
    "clean": "rimraf lib tsconfig.tsbuildinfo",
    "prepublishOnly": "npm run clean && npm run build",
    "watch": "tsc --build --watch --listEmittedFiles"
  },
  "dependencies": {
    "@jupyterlab/dataregistry": "^2.0.0",
    "@jupyterlab/dataregistry-csvviewer": "0.0.0",
    "@jupyterlab/dataregistry-extension": "^2.2.2",
    "@jupyterlab/dataregistry-registry": "0.0.0"
  },
  "devDependencies": {
    "rimraf": "~2.6.2",
    "typescript": "3.5.3"
  },
  "publishConfig": {
    "access": "public"
  }
}
