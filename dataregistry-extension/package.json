{
  "name": "@jupyterlab/dataregistry-extension",
  "version": "2.2.1",
  "description": "Datasets as first class entities in Juptyerlab.",
  "keywords": [
    "jupyter",
    "jupyterlab",
    "jupyterlab-extension"
  ],
  "homepage": "https://github.com/jupyterlab/jupyterlab-data-explorer",
  "bugs": {
    "url": "https://github.com/jupyterlab/jupyterlab-data-explorer/issues"
  },
  "license": "BSD-3-Clause",
  "author": "Project Jupyter",
  "files": [
    "lib/**",
    "style.css"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "style": "style.css",
  "directories": {
    "lib": "lib/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jupyterlab/jupyterlab-data-explorer.git"
  },
  "scripts": {
    "build": "tsc && yarn run generateSchema",
    "clean": "rimraf lib tsconfig.tsbuildinfo",
    "prepublishOnly": "yarn run clean && yarn run build",
    "watch": "tsc -w --listEmittedFiles",
    "lint": "eslint src/*",
    "generateSchema": "ts-json-schema-generator --type datasetsObjectType --no-type-check -f tsconfig.json > lib/datasets-file.schema.json"
  },
  "dependencies": {
    "@jupyterlab/application": "^1.0.0",
    "@jupyterlab/apputils": "^1.0.0",
    "@jupyterlab/coreutils": "^3.0.0",
    "@jupyterlab/csvviewer": "^1.0.0",
    "@jupyterlab/dataregistry": "^2.0.0",
    "@jupyterlab/docmanager": "^1.0.0",
    "@jupyterlab/docregistry": "^1.0.4",
    "@jupyterlab/filebrowser": "^1.0.0",
    "@jupyterlab/notebook": "^1.0.0",
    "@jupyterlab/rendermime": "^1.0.0",
    "@nteract/data-explorer": "7.0.1",
    "@phosphor/datagrid": "^0.1.10",
    "@phosphor/messaging": "^1.2.3",
    "@phosphor/widgets": "^1.8.0",
    "ajv": "6.10.2",
    "js-yaml": "3.13.1",
    "path": "0.12.7",
    "react": "16.8.4",
    "react-inspector": "3.0.0",
    "rxjs": "6.5.2",
    "styled-components": "4.3.2"
  },
  "devDependencies": {
    "@types/js-yaml": "3.12.1",
    "@typescript-eslint/eslint-plugin": "1.13.0",
    "@typescript-eslint/parser": "1.13.0",
    "eslint": "6.1.0",
    "eslint-config-prettier": "6.0.0",
    "eslint-plugin-react": "7.14.3",
    "eslint-plugin-react-hooks": "1.6.1",
    "rimraf": "~2.6.2",
    "ts-json-schema-generator": "0.52.14",
    "typescript": "3.5.3"
  },
  "publishConfig": {
    "access": "public"
  },
  "jupyterlab": {
    "extension": true
  }
}
